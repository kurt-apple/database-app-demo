<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minWidth="580.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="softwareiiredux.FXMLNewAppointmentController">
   <children>
      <HBox minWidth="550.0">
         <children>
            <Label contentDisplay="CENTER" minHeight="25.0" minWidth="110.0" text="TITLE" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets left="10.0" top="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="title" minWidth="205.0" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="10.0" top="10.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox minWidth="550.0">
         <children>
            <Label contentDisplay="CENTER" minHeight="25.0" minWidth="110.0" text="CUSTOMER NAME" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets left="10.0" top="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="custname" minWidth="150.0" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="10.0" top="10.0" />
               </HBox.margin>
            </TextField>
            <Button fx:id="findButton" maxWidth="45.0" minWidth="45.0" mnemonicParsing="false" onMouseClicked="#handleFind" text="FIND" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
            <TextField fx:id="custid" maxWidth="60.0" minWidth="20.0">
               <HBox.margin>
                  <Insets right="10.0" top="10.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox minWidth="550.0">
         <children>
            <Label contentDisplay="CENTER" minHeight="25.0" minWidth="110.0" text="DESCRIPTION" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets left="10.0" top="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="descr" minWidth="205.0" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="10.0" top="10.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox minWidth="550.0">
         <children>
            <Label contentDisplay="CENTER" minHeight="25.0" minWidth="110.0" text="LOCATION" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets left="10.0" top="10.0" />
               </HBox.margin>
            </Label>
            <ChoiceBox fx:id="location" prefWidth="450.0" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="10.0" top="10.0" />
               </HBox.margin>
            </ChoiceBox>
         </children>
      </HBox>
      <HBox minWidth="550.0">
         <children>
            <Label contentDisplay="CENTER" minHeight="25.0" minWidth="110.0" text="CONTACT" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets left="10.0" top="10.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="contact" minWidth="205.0" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="10.0" top="10.0" />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox minWidth="550.0">
         <children>
            <Label contentDisplay="CENTER" minHeight="25.0" minWidth="110.0" text="START" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets left="10.0" top="10.0" />
               </HBox.margin>
            </Label>
            <DatePicker fx:id="startdate">
               <HBox.margin>
                  <Insets right="10.0" top="10.0" />
               </HBox.margin>
            </DatePicker>
            <TextField fx:id="starttime" prefWidth="200.0" promptText="HH:MM" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="10.0" top="10.0" />
               </HBox.margin>
            </TextField>
            <ChoiceBox fx:id="AMPMstart" minWidth="55.0" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets right="10.0" top="10.0" />
               </HBox.margin>
            </ChoiceBox>
         </children>
      </HBox>
      <HBox minWidth="550.0">
         <children>
            <Label contentDisplay="CENTER" minHeight="25.0" minWidth="110.0" text="END" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets left="10.0" top="10.0" />
               </HBox.margin>
            </Label>
            <DatePicker fx:id="enddate">
               <HBox.margin>
                  <Insets right="10.0" top="10.0" />
               </HBox.margin>
            </DatePicker>
            <TextField fx:id="endtime" prefWidth="200.0" promptText="HH:MM" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="10.0" top="10.0" />
               </HBox.margin>
            </TextField>
            <ChoiceBox fx:id="AMPMend" minWidth="55.0" HBox.hgrow="NEVER">
               <HBox.margin>
                  <Insets right="10.0" top="10.0" />
               </HBox.margin>
            </ChoiceBox>
         </children>
      </HBox>
      <HBox maxWidth="580.0" nodeOrientation="RIGHT_TO_LEFT">
         <children>
            <Button fx:id="cancelButton" minWidth="70.0" mnemonicParsing="false" onMouseClicked="#handleCancel" text="CANCEL">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="saveButton" minWidth="70.0" mnemonicParsing="false" onMouseClicked="#handleSave" text="SAVE">
               <HBox.margin>
                  <Insets bottom="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="deleteButton" minWidth="70.0" mnemonicParsing="false" onMouseClicked="#handleDelete" text="DELETE">
               <HBox.margin>
                  <Insets top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
