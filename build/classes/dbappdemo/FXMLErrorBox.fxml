<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="softwareiiredux.FXMLErrorBoxController">
   <children>
      <Label fx:id="boxMessage" alignment="CENTER" minHeight="70.0" minWidth="200.0" text="DEFAULT ERROR NOMSG" textAlignment="CENTER">
         <padding>
            <Insets left="20.0" right="20.0" />
         </padding></Label>
      <Button fx:id="okButton" minHeight="20.0" minWidth="40.0" mnemonicParsing="false" onMouseClicked="#handleOkButtonAction" text="OK">
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
